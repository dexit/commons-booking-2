{
    "require": {
        "php": ">=5.6",
        "composer\/installers": "1.x",
        "mnsami\/composer-custom-directory-installer": "1.1.*",
        "wpbp\/pointerplus": "1.0.0",
        "wpbp\/cronplus": "1.0.3",
        "wpbp\/widgets-helper": "1.0.3",
        "wpbp\/fakepage": "1.0.0",
        "wpbp\/template": "1.0.2",
        "wpbp\/debug": "1.0.1",
        "wpbp\/cpt_columns": "1.0.1",
        "wpbp\/requirements": "1.0.0",
        "wpbp\/language": "1.0.0",
        "wpbp\/backbone-modal-view": "1.0.2",
        "webdevstudios\/cmb2": "2.3.0",
        "kevinlangleyjr\/wp-contextual-help": "dev-master",
        "nathanielks\/wp-admin-notice": "dev-master",
        "origgami/cmb2-grid": "dev-master",
        "rubengc\/cmb2-tabs": "*",
        "johnbillion\/extended-cpts": "4.2",
				"julien731\/wp-dismissible-notices-handler": "1.0.0",
				"azuyalabs\/yasumi": "1.8.0",
        "opencage/geocode": "dev-master"
    },
    "repositories": [
        {
            "url": "git@github.com:nathanielks\/wordpress-admin-notice.git",
            "type": "git"
        },
        {
            "type": "package",
            "package": {
                "name": "rubengc\/cmb2-tabs",
                "version": "1.0.0",
                "source": {
                    "url": "https:\/\/github.com\/rubengc\/cmb2-tabs",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "composer",
            "url": "https:\/\/wpackagist.org"
        }
    ],
    "config": {
        "vendor-dir": "composer\/"
    },
    "require-dev": {
        "phpro\/grumphp": "^0.12",
        "lucatume\/function-mocker": "~1.0"
    },
    "minimum-stability": "dev",
    "extra": {
        "installer-paths": {
            ".\/includes\/lib\/{$name}": [
                "johnbillion\/extended-cpts",
                "wpbp\/language",
                "wpbp\/cronplus",
                "wpbp\/fakepage",
                "wpbp\/template",
                "wpbp\/requirements",
                "wpbp\/debug",
								"wpackagist-plugin\/posts-to-posts",
								"azuyalabs\/yasumi"
            ],
            ".\/includes\/lib\/Freemius": [
                "freemius\/wordpress-sdk"
            ],
            ".\/public\/includes\/lib\/{$name}": [
                "wpbp\/widgets-helper"
            ],
            ".\/admin\/includes\/lib\/{$name}": [
                "webdevstudios\/cmb2",
                "origgami\/cmb2-grid",
                "rubengc\/cmb2-tabs",
                "wpbp\/pointerplus",
                "wpbp\/cpt_columns",
                "wpbp\/backbone-modal-view",
                "kevinlangleyjr\/wp-contextual-help",
                "nathanielks\/wp-admin-notice",
                "julien731\/wp-dismissible-notices-handler"
            ]
        }
    }
}
